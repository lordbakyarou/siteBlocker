{
  "manifest_version": 3,
  "name": "Site Blocker",
  "version": "1.0",
  "description": "Block unwanted sites",
  "permissions": ["storage", "webNavigation"],
  "icons": {
    "48": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
